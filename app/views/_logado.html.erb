<div id="logado">
  <% if user_signed_in? %>
    Logado como <strong><%= current_user.email %></strong>.
    <%= link_to "Sair", destroy_user_session_path, 'data-turbo-method': :delete, :class => 'navbar-link text-decoration-none'  %>
  <% else %>
    <%= link_to "Cadastrar", new_user_registration_path, :class => 'navbar-link text-decoration-none'  %> |
    <%= link_to "Entrar", new_user_session_path, :class => 'navbar-link text-decoration-none'  %>
  <% end %>
</div>
